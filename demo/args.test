/* Использование аргументов функций. */

int main() {
  f2(10, f1(10, 20), 99);
  return 0;
}

int f1(int a, int b) {
  int count;
  puts("в функции f1");
  count = a;
  do {
    print(count);
  } while(count=count-1);
  print(a);
  print(b);
  print(a*b);
  puts("");
  return a*b;
}

int f2(int a, int x, int y) {
  puts("в функции f2");
  print(a);
  print(x);
  print(x / a);
  print(y*x);
  puts("");
  return 0;
}

